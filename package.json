{
  "name": "node-version-utils",
  "version": "0.5.7",
  "description": "Utilities for running commands on a specific version of node by installed path",
  "keywords": [
    "node",
    "version",
    "spawn",
    "set",
    "install",
    "path",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-utils.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --timeout=20000",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "core-js": "^3.39.0",
    "cross-spawn-cb": "^1.1.0",
    "env-path-key": "^1.0.0",
    "path-string-prepend": "^0.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "is-version": "^0.2.1",
    "node-install-release": "^1.4.0",
    "node-resolve-versions": "^1.0.0",
    "ts-dev-stack": "^0.16.3"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
