{
  "name": "node-version-utils",
  "version": "1.0.2",
  "description": "Utilities for running commands on a specific version of node by installed path",
  "keywords": [
    "node",
    "version",
    "spawn",
    "set",
    "install",
    "path",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-utils.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^1.3.0",
    "env-path-key": "^1.0.0",
    "path-string-prepend": "^1.0.0",
    "pinkie-promise": "^2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "is-version": "^0.2.1",
    "node-install-release": "^1.4.4",
    "node-resolve-versions": "^1.0.0",
    "rimraf2": "^2.8.2",
    "ts-dev-stack": "^1.5.3"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
